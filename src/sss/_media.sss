@media screen and (max-width: 1000px)
	html
		font-size: 94%

	.wrapper
		padding: 0 18px

	.burger
		position: relative
		size: 30px 20px
		display: flex
		flex-direction: column
		justify-content: space-between
		cursor: pointer

		&:after, &:before
			content: ""
			transform-origin: left
			display: inline-block

		&:matches(:after, :before, & span)
			background: var(--mainColor)
			height: 2px

		span
			transition: opacity .3s ease

	body

		&.js__filter-opened

			.filter
				transform: translate3d(100%, 0, 0)

		&.js__menu-opened, &.js__filter-opened
			overflow: hidden
			position: fixed 0 0 * 0

	body.js__menu-opened

		.head, .head__mobile-menu, .footer, #content
			transform: translate3d(-270px, 0, 0) !important

	.head, .head__mobile-menu, .footer, #content
		transition: transform .3s ease

	.head

		&__nav
			display: none

		&__wrap
			padding: 10px 0

		&__mobile-menu
			position: fixed 0 * 0 100%
			overflow-y: auto
			-webkit-overflow-scrolling: touch
			overflow-scrolling: touch
			border-left: 1px solid #e7e7e7
			z-index: 10
			background: var(--secondColor)
			width: 270px

		&__burger

			&.js__active
				
				.burger
					width: 25px

					span
						opacity: 0

					&:before
						transform: rotate(45deg)

					&:after
						transform: rotate(-45deg)

	.mobile-menu
		padding: 20px 0

		&__search
			padding: 0 15px 20px

			.search-form
				border: 1px solid #e7e7e7

				&__text
					padding: 0 15px
					flex: none
					width: calc(100% - 46px)

		&__list
			border-top: 1px solid #e7e7e7

		&__link
			display: block
			padding: 15px

		&__item
			border-bottom: 1px solid #e7e7e7

		&__info
			padding: 30px 15px

			&-download, &-phone, &-mail, &-address

				&:not(:first-child)
					margin: 20px 0 0

			.dl-btn
				width: 100%

	.head-bot

		&__title
			display: none

	.main-catalog
		flex-direction: column
		padding-bottom: 1px

		&__aside, &__right
			width: auto

		&__list
			margin: 0

	.main-cat
		flex-direction: row
		flex-wrap: wrap

		&__item
			width: 50%

			&:nth-child(odd)

				&:not(:last-child)
					padding-right: 1px

		&__link

			&.have-sub

				&:after
					transform: rotate(135deg)

	.main-cat-block
		margin: 0 0 -1px -1px

		&__item
			width: calc((100% - 1px * 2) / 2)
			margin: 0 0 1px 1px

	.main-content
		margin: 0

		> .wrapper
			padding: 0

	.main-sliders

		&__cont
			padding: 20px 0 50px

		&__title

			&-cont
				justify-content: center
				margin-bottom: 18px

	.stock-slider

		.slick-arrow
			display: none

		.swiper-pagination
			bottom: -34px

			&-bullet
				margin: 0 !important
				border-radius: 50%
				position: relative
				width: 18px
				height: 18px
				border-radius: 50%
				overflow: hidden
				background: none
				text-indent: -9999px
				color: var(--mainColor)
				transition: border-color .3s ease
				border: 1px solid transparent
				opacity: 1

				&:before
					content: ""
					top: 50%
					left: 50%
					position: absolute
					transform: translate3d(-50%, -50%, 0)
					width: 11px
					height: 11px
					border-radius: inherit
					background: currentColor
					transition: transform .3s ease

				&-active
					border-color: currentColor

	.main-news
		padding: 20px 0
		flex-direction: column

		&__title

			&-cont
				margin-bottom: 10px

		&__big, &__list
