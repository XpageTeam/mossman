	.promokod

		form
			flex-direction: column-reverse
			align-items: flex-start

		&__input, &__label
			width: 100%

		&__label
			margin-bottom: 10px

	.cart-itog
		text-align: left

	.cart__btns
		flex-direction: column

		.arrow-btn
			margin-right: 0 !important

			&:not(:first-child)
				margin: 18px 0 0

	.cart-table
		display: flex
		flex-direction: column

		tfoot
			order: 1
			margin: 30px 0 0

			tr
				flex-direction: column

			td

				&:last-child
					padding-top: 0

		tbody, tfoot, td
			display: block

		thead
			display: none

		tr
			display: flex
			flex-wrap: wrap
			position: relative

			&:not(:first-child)
				margin: 30px 0 0

		tbody

			tr:last-child

				td
					border-bottom: 1px solid var(--borderColor)

		&__remove
			position: absolute 0 0 * *
			padding: 0 !important
			border: none !important

			.remove
				text-indent: 0
				overflow: visible
				color: var(--mainColor)
				background: none
				size: auto

		&__img
			width: 35%
			padding-top: 0 !important

		&__name
			width: 65%
			padding-right: 0 !important
			padding-top: 30px !important
			max-width: none

		&__price, &__count, &__total
			width: 100%
			padding-right: 0 !important

		td

			&[data-label]
				display: flex
				align-items: center

				&:before
					content: attr(data-label)
					display: inline-block
					padding-right: 16px
					width: 38%

	.cart-item

		&__img
			width: auto

	.stocks, .catalog
		margin: 0

		&__item
			width: 100%
			margin: 0

			&:not(:first-child)
				margin: 16px 0 0

	.catalog

		&__item

			.main-cat-block

				&__item

					&-desc
						padding: 20px 50px 20px 25px

	.history

		&:after
			display: none

		&__img
			float: none
			width: 100%
			max-width: none

			img
				width: 100%

	.timeline
		
		&__title

			&-cont

		&__arrows
			display: none

	.time-slide

		&__text
			padding: 20px 18px

	.head
		display: flex

		> .wrapper
			flex: 1 1 auto
			padding-left: 0

		&__bot
			order: -1
			background: none
			color: #000
			display: flex
			align-items: center

		&__download
			display: none

		&__lc
			order: -1

	.head-cart

		&__link
			background: url("../img/ico-cart-blue.png") no-repeat

			&:before
				color: #fff !important
				background: var(--mainColor) !important

	.head-lc

		&__link
			margin-left: 0
			display: block
			overflow: hidden
			text-indent: -9999px
			size: width("../img/ico-lc.png") height("../img/ico-lc.png")
			background: url("../img/ico-lc.png") no-repeat

			&:before
				display: none
		


	.logo

		img, svg
			display: block

	.head-bot

		&__search
			display: none

	.main-cat
		flex-direction: column

		&__item
			width: 100%

	.main-slider__cont
		display: none

	.main-cat-block
		margin: 1px 0 0
		flex-direction: column

		&__item
			width: auto

			&-desc
				padding: 15px 100px 15px 15px

			&-title

				&:after
					bottom: 15px
					left: 15px

	.big-news__desc, .small-news
		padding: 18px 40px 18px 18px

		&:after
			right: 20px

	.big-news

		&__img

			&-date
				left: 18px
				top: 18px

		&__title
			font-size: 14px

	.main-sliders

		&__switcher
			margin: 0 5px

	.footer

		&__top, &__address
			display: none

		&__bot
			justify-content: center
			padding: 15px 0

	.news-item
		flex-direction: column
		align-items: stretch

		&__img
			margin-right: 0
			margin-bottom: 1rem

			img
				width: 100%

	.text-page

		[align="right"], [align="left"]
			margin: 0 0 20px 0
			max-width: none
			float: none

@media screen and (max-width: 400px)
	.deps

		&__item
			width: 100%
			padding-bottom: 20px
			align-items: flex-start
			border: none !important
			border-bottom: 1px solid var(--borderColor) !important

	.dep

		&__title
			margin-bottom: 12px